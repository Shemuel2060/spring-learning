package com.shemuel.spring.stereotypes;

import org.springframework.stereotype.Service;

@Service
public class CarServices {
	
	private Car car;
	
	
	public void printService() {
		System.out.println("Servicing "+ car.getEngine());
	}
	
	
	public Car getCar() {
		return car;
	}
	public void setCar(Car car) {
		this.car = car;
	}

}
